{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "plugin:react-hooks/recommended"
  ],
  "plugins": ["react", "react-hooks"],
  "rules": {
    "react/jsx-no-target-blank": "off",
    "react/prop-types": "off"
  },
  "settings": {
    "react": {
      "version": "18.3"
    }
  }
}
